---
layout: main
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /tags/{{ tag }}/
---

<h1> Tagged "{{ tag }}"</h1>

<ul>
    {% assign tagList = collections[ tag ] %}
    {% for post in tagList | reverse %}
        <li>
            <a href="{{ post.url }}">{{ post.data.title }}</a>
        </li>
    {% endfor %}
</ul>
